{
  "bindings": [
    {
      "type": "httpTrigger",
      "name": "req",
      "direction": "in",
      "methods": [
        "get",
        "post"
      ],
      "authLevel": "anonymous",
      "route": "{surName}/{lastName}/{vehicleCode}/{km}/{transactionID}/{vehicleDescription}"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "databaseIN",
      "type": "cosmosDB",
      "databaseName": "juri_km",
      "collectionName": "km_reg",
      "connectionStringSetting": "connectionStringSetting",
      "sqlQuery": "SELECT * FROM c where LOWER(CONCAT(c.surName, c.lastName, c.vehicleCode)) = LOWER(CONCAT({surName}, {lastName}, {vehicleCode})) ORDER BY c.timeID desc offset 0 LIMIT 1",
      "direction": "in"
    },
    {
      "name": "workersOUT",
      "type": "cosmosDB",
      "databaseName": "juri_km",
      "collectionName": "km_reg",
      "createIfNotExists": true,
      "connectionStringSetting": "connectionStringSetting",
      "direction": "out"
    }
  ]
}